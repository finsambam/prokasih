<% @analytics.each do |a| %>
  <tr>
    <td>
      <span><%= "#{a.location.river_name}" %></span>
      <div class="btn-group" style="float: right;">
        <button type="button" class="btn btn-default dropdown-toggle" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
          <span class="caret"></span>
        </button>
        <ul class="dropdown-menu">
          <li><%= link_to_if(can?(:edit, Analytic), "Ubah", edit_analytic_path(a.id)) {} %></li>
          <li role="separator" class="divider"></li>
          <li><%= link_to_if(can?(:show, Analytic), "Detail", analytic_path(a.id)) {} %></li>
          <li role="separator" class="divider"></li>
          <li><%= link_to_if(can?(:delete, a), "Hapus", a, :data => {:confirm => 'Apakah anda yakin?'}, :method => :delete) {} %></li>
        </ul>
      </div>
    </td>
    <td>  
      <span><%= "#{a.location.spot_name}" %></span>
      <span><%= "#{a.location.address}" %></span>

    </td>
    <td><%= a.period.strftime("%B %Y ") %></td>
  </tr>
<% end %>