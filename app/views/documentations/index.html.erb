<div class="header-content">
  <h2 class="page-header">Dokumentasi</h2>
</div>
<div class="row">
  <% if @documentations.any? %>
    <% @documentations.each do |doc| %>
      <div class="col-xs-6 col-md-3">
        <%= link_to(image_tag(doc.image_url(:thumb)), documentation_path(doc.id), :remote => true, :class => 'thumbnail') %>
      </div>
    <% end %>
  <% else %>
    <%= render :partial => 'shared/no-data'%>
    <!-- <table id="empty-result" class="table table-striped table-bordered">
      <tr>
        <td>Data tidak ditemukan</td>
      </tr>
    < /table>-->
  <% end %>
</div>
<%= will_paginate @articles, :page_links => false, renderer: BootstrapPagination::Rails %>  
<div id="preview-container">
</div>