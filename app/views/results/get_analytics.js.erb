<% if @analytics.any? %>
  $("#download-pdf").removeClass("disabled");
  $('#download-pdf').prop('disabled', false);
  $('a#download-pdf').removeAttr("disabled");
<% else %>
  $("#download-pdf").addClass("disabled");
  $('#download-pdf').prop('disabled', true);
  $('a#download-pdf').attr("disabled", true);
  $('a#download-pdf').click(function(event){
    event.preventDefault();
  });
<% end %>
$('#result-container').html('<%= escape_javascript (render "result_table") %>');